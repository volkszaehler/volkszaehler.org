{
    "name": "volkszaehler/volkszaehler",
    "type": "project",
    "description": "volkszaehler is a free smart meter implementation with focus on data privacy",
    "homepage": "http://www.volkszaehler.org/",
    "license": "GPL-3.0",
    "support": {
        "wiki": "http://wiki.volkszaehler.org/",
        "email": "volkszaehler@lists.volkszaehler.org"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.0",
        "doctrine/orm": "^2.5",
        "doctrine/dbal": "^2.5",
        "doctrine/cache": "^1.6",
        "andig/dbcopy": "^1.1",
        "symfony/console": "^2.6|^3.0",
        "symfony/http-kernel": "^2.6|^3.0",
        "symfony/http-foundation": "^2.6|^3.0",
        "symfony/routing": "^2.6|^3.0",
        "react/socket": "^0.8.6",
        "cboden/ratchet": "^0.4",
        "react/http": "dev-master",
        "php-pm/php-pm": "dev-react-0.8",
        "php-pm/httpkernel-adapter": "dev-react-0.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.4|^5.0",
        "react/http-client": "^0.4",
        "ringcentral/psr7": "^1.2",
        "symfony/psr-http-message-bridge": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "zendframework/zend-diactoros": "^1.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/andig/php-pm"
        },
        {
            "type": "vcs",
            "url": "https://github.com/andig/php-pm-httpkernel"
        }
    ],
    "autoload": {
        "psr-4": {
            "Volkszaehler\\": "lib/",
            "Tests\\": "test/"
        }
    },
    "suggest": {
        "jpgraph/jpgraph": "Server-side chart generation for volkszaehler"
    }
}
